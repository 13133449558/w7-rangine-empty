{
	"name": "w7/rangine-demo",
	"type": "project",
	"description": "基于 Swoole 的常驻内存型 PHP 高性能框架",
	"license": "Apache-2.0",
	"keywords": [
		"we7",
		"swoole"
	],
	"homepage": "https://www.rangine.com/",
	"authors": [
		{
			"name": "we7team",
			"email": "donknap@gmail.com"
		}
	],
	"autoload": {
		"psr-4": {
			"W7\\App\\": "app/",
			"W7\\Tests\\": "tests/"
		},
		"files": [
			"config/define.php",
			"app/Helper/Functions.php"
		],
		"classmap": [
			"database/seeds",
			"database/migrations"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"W7\\Tests\\" : "tests/"
		}
	},
	"repositories": [
		{
			"type": "git",
			"url": "https://github.com/we7coreteam/w7-rangine-fpm.git"
		}
	],
	"require": {
		"w7/rangine": "dev-master",
		"w7/rangine-http-message": "dev-master",
		"w7/rangine-fpm": "dev-master",
		"w7/rangine-crontab": "~1.0",
		"w7/rangine-client": "~1.0",
		"doctrine/dbal": "*"
	},
	"require-dev": {
		"w7/rangine-debugger": "~1.0.0",
		"phpunit/phpunit": "^7.0"
	}
}