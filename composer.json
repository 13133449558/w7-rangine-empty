{
	"name": "w7/rangine",
	"type": "project",
	"description": "基于 Swoole 的常驻内存型 PHP 高性能框架",
	"license": "Apache-2.0",
	"keywords": [
		"we7",
		"swoole"
	],
	"homepage": "https://www.rangine.com/",
	"authors": [
		{
			"name": "we7team",
			"email": "donknap@gmail.com"
		}
	],
	"repositories": [
		{
			"type" : "composer",
			"url" : "https://packagist.rangine.com/"
		}
	],
	"autoload": {
		"psr-4": {
			"W7\\App\\": "app/",
			"W7\\Tests\\" : "tests/"
		},
		"files": [
			"config/define.php",
			"app/Helper/Functions.php"
		]
	},
	"require": {
		"w7/swoole": "dev-master",
		"w7/http-message": "dev-master",
		"w7/laravel-cache-model" : "dev-master",
		"phpunit/phpunit": "^8.0"
	},
	"require-dev": {
		"eaglewu/swoole-ide-helper": "dev-dev"
	}
}
